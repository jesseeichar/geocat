        <addInterceptUrl  pattern="/srv/\w\w\w/reusable.list.js!?.*" access="hasRole('Administrator')"/>
        <addInterceptUrl  pattern="/apps/shared-objects/app/.*" access="hasRole('Administrator')"/>

        <addInterceptUrl  pattern="/srv/\w\w\w/metadata.summary.zip!?.*" access="hasRole('Administrator')"/>
        <addInterceptUrl  pattern="/srv/\w\w\w//srv/\w\w\w/inspire.edit!?.*" access="hasRole('Editor')"/>
